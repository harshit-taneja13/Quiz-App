{\n  "name": "quiz-app",\n  "version": "1.0.0",\n  "description": "A full-stack quiz application with React frontend and Node.js backend",\n  "main": "server/index.js",\n  "scripts": {\n    "start": "node server/index.js",\n    "dev": "concurrently \"npm run server\" \"npm run client\"",\n    "server": "nodemon server/index.js",\n    "client": "cd client && npm start",\n    "build": "cd client && npm run build",\n    "install-client": "cd client && npm install",\n    "install-server": "npm install",\n    "install-all": "npm run install-server && npm run install-client"\n  },\n  "dependencies": {\n    "express": "^4.18.2",\n    "mongoose": "^7.5.0",\n    "cors": "^2.8.5",\n    "dotenv": "^16.3.1",\n    "bcryptjs": "^2.4.3",\n    "jsonwebtoken": "^9.0.2"\n  },\n  "devDependencies": {\n    "nodemon": "^3.0.1",\n    "concurrently": "^8.2.0"\n  },\n  "keywords": ["quiz", "react", "nodejs", "mongodb"],\n  "author": "harshit-taneja13",\n  "license": "MIT"\n}